{
  "name": "harbour.js",
  "version": "0.0.1",
  "description": "Harbour API",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "build": "webpack",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jasmine"
  },
  "keywords": [
    "harbour",
    "ethereum",
    "blockchain",
    "governance"
  ],
  "author": "Harbour",
  "license": "GNU-GPL",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.5.0",
    "eslint-watch": "^2.1.14",
    "jasmine": "^2.7.0",
    "npm-run-all": "^4.0.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.7.1"
  }
}
